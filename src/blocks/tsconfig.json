{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noUncheckedIndexedAccess": false
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.*",
    "**/*.spec.*"
  ]
}