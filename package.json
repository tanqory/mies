{
  "name": "@tanqory/mies",
  "version": "1.0.0",
  "description": "A comprehensive, accessible, and customizable React component library built with TypeScript, Tailwind CSS, and Radix UI primitives.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./theme": {
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup && tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "chromatic": "npx chromatic --project-token=YOUR_PROJECT_TOKEN"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript",
    "tailwindcss",
    "radix-ui",
    "accessible",
    "tanqory"
  ],
  "author": "Tanqory Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanqory/mies.git"
  },
  "bugs": {
    "url": "https://github.com/tanqory/mies/issues"
  },
  "homepage": "https://github.com/tanqory/mies#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
